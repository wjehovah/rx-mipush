<prescriptions xmlns="http://greenify.github.io/schemas/prescription/v3">
    <prescription sender="other" type="broadcast">
        <intent-filter>
            <action android:name="cn.jpush.android.service.PushReceiver" />
            <action android:name=" cn.jpush.android.service.AlarmReceiver" />
        </intent-filter>
    </prescription>
    <prescription sender="other" type="service">
        <intent-filter>
            <action android:name="cn.jpush.android.service.PushService" />
            <action android:name="cn.jpush.android.service.DaemonService" />
            <action android:name="cn.jpush.android.service.DownloadService" />
        </intent-filter>
    </prescription>

    <prescription sender="other" type="activity">
        <intent-filter>
            <action android:name="cn.jpush.android.ui.PushActivity" />
            <action android:name="cn.jpush.android.ui.PopWinActivity" />
        </intent-filter>
    </prescription>
    <!--以上极光推送 参考极光集成文档 https://docs.jiguang.cn/jpush/client/Android/android_guide/-->
    <prescription sender="other" type="broadcast">
        <intent-filter>
            <action android:name="com.igexin.sdk.PushReceiver" />
            <action android:name="com.igexin.download.DownloadReceiver" />
        </intent-filter>
    </prescription>
    <prescription sender="other" type="service">
        <intent-filter>
            <action android:name="com.igexin.sdk.PushService" />
            <action android:name="com.igexin.download.DownloadService" />
        </intent-filter>
    </prescription>

    <prescription sender="other" type="activity">
        <intent-filter>
            <action android:name="com.igexin.sdk.PushActivity" />
            <action android:name="com.igexin.sdk.GActivity" />
        </intent-filter>
    </prescription>
    <!--以上个推 参考个推集成文档 http://docs.getui.com/mobile/android/androidstudio/#3-sdk-->

    <prescription sender="other" type="broadcast">
        <intent-filter>
            <action android:name="com.alibaba.sdk.android.push.MsgService" />
            <action android:name="com.alibaba.sdk.android.push.channel.TaobaoRecvService" />
        </intent-filter>
    </prescription>
    <prescription sender="other" type="service">
        <intent-filter>
            <action android:name="com.alibaba.sdk.android.push.ChannelService" />
        </intent-filter>
    </prescription>

    <!--以上阿里云 参考极光集成文档 https://help.aliyun.com/document_detail/30064.html?spm=5176.doc30056.2.2.2pc23z-->

    <prescription sender="other" type="broadcast">
        <intent-filter>
            <action android:name="com.taobao.accs.EventReceiver" />
            <action android:name="com.taobao.accs.ServiceReceiver" />
            <action android:name="com.taobao.agoo.AgooCommondReceiver" />
            <action android:name="com.umeng.message.NotificationProxyBroadcastReceiver" />
        </intent-filter>
    </prescription>
    <prescription sender="other" type="service">
        <intent-filter>
            <action android:name="com.taobao.accs.ChannelService" />
            <action android:name="com.taobao.accs.data.MsgDistributeService" />
            <action android:name="com.taobao.accs.ChannelService$KernelService" />
            <action android:name="org.android.agoo.accs.AgooService" />
            <action android:name="com.umeng.message.UmengIntentService" />
            <action android:name="com.umeng.message.XiaomiIntentService" />
            <action android:name="com.umeng.message.UmengMessageIntentReceiverService" />
            <action android:name="com.umeng.message.UmengMessageCallbackHandlerService" />
            <action android:name="com.umeng.message.UmengDownloadResourceService" />
        </intent-filter>
    </prescription>

    <!--以上友盟推送-->
</prescriptions>
